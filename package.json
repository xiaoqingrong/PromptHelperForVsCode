{
  "name": "prompt-helper-frontend",
  "displayName": "prompt-helper-frontend",
  "description": "prompt-helper-frontend",
  "version": "0.0.2",
  "publisher": "PromptHelper",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoqingrong/PromptHelperForVsCode.git"
  },
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.74.0"
	},
  "keywords": [
    "frontend",
    "ai",
    "prompt",
    "前端",
    "提示词",
    "前端提示词",
    "前端提示词生成器"
  ],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"frontAI": [
				{
					"type": "webview",
					"id": "calicoColors.colorsView",
					"name": "",
          "icon": "images/icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "calicoColors.addColor",
				"category": "Calico Colors",
				"title": "Add Color"
			},
			{
				"command": "calicoColors.clearColors",
				"category": "Calico Colors",
				"title": "Clear Colors",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				}
			]
		},
    "viewsContainers": {
      "activitybar": [
        {
          "id": "frontAI",
          "title": "前端AI提示词",
          "icon": "images/icon.svg"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
	}
}
